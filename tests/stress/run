cmd default() {
  echo "$doc"
}

/// Generates a very long file to test parsing speed
py cmd gen_long() {{{
  with open("stress-long.run", "w") as file:
    file.write("cmd default() { echo 'Testing parsing a very long file' }\n")
    for i in range(1000000):
      file.write(f"cmd {i}() {{ echo {i} }}\n")
}}}