fn default() {
  echo -e "Hello World in multiple languages\n"
  echo -e "Usage: <COMMAND> <NAME>\n"
  echo "Commands: sh, py, js, rs"
}

# Example hello world in bash
# $usage
sh fn sh(name) {
  echo "Hello, $name.sh";
}

py fn py(name) {
  print("Hello, $name.py")
}

js fn js(name) {
  console.log("Hello, $name.js");
}

rs fn rs(name) {
  println!("Hello, $name.rs");
}